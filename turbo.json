{
    "$schema": "https://turborepo.org/schema.json",
    "tasks": {
      "dev": {
        "dependsOn": ["^dev"],
        "cache": false,
        "persistent": true
      },
      "build": {
        "dependsOn": ["^build"],
        "outputs": [".next/**", "dist/**"]
      },
      "lint": {
        "outputs": []
      }
    }
  }
  